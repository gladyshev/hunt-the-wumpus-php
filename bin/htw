#!/usr/bin/env php
<?php

chdir(__DIR__ . '/..');

/** @var \Psr\Container\ContainerInterface $container */
$container = require 'bootstrap/container.php';

try {
    $main = $container->get(\Htw\Cli\Main::class);
    $main->start(1, 'Hunter');
} catch (\Throwable $e) {
    fwrite(\STDERR, (string) $e);
    return 0;
}

return 0;
